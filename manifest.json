{
    "manifest_version": 2,
    "name": "detach-bilibli",
    "version": "1.0.2",
    "description": "bilibili master",
    "content_scripts": [{
        "css": ["css/nav.css"],
        "js": [
            "js/jq-min-1.3.0.js", "content.js","js/lodash.js"
        ],
        "matches": ["https://www.bilibili.com/", "http://www.bilibili.com/"],


        "run_at": "document_start"
    }],
    "permissions": ["tabs"]

}